# This file is used to override settings from needrestart.conf_main.
# Overrides are managed by puppet

$nrconf{override_rc} = {
<% if @ignorelist -%>
  <% @ignorelist.each do | name, services | -%>
        #<%= name %>
  <% services.sort.each do | service | -%>
      q(^<%= service %>) => 0,
  <% end -%>
  <% end -%>
<% end -%>
};
